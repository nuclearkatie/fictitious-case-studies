            <leftover_commod>extra_fluorine</leftover_commod>
            <leftoverbuf_size>1e199</leftoverbuf_size>
            <streams>
                <item>
                    <commod>enriched_U_fuelfab_prestore</commod>
                    <info>
                        <buf_size>14532</buf_size>
                        <efficiencies>
                            <item>
                                <comp>U</comp>
                                <eff>0.99</eff>
                            </item>
                        </efficiencies>
                    </info>
                </item>
            </streams>
        </Separations>
    </config>
</facility>

<facility>
    <name>FuelFab</name>
    <config>
        <Mixer>
            <in_streams>
                <stream>
                    <info>
                        <mixing_ratio>0.1185</mixing_ratio>
                        <buf_size>1e4</buf_size>
                    </info>
                    <commodities>
                        <item>
                            <commodity>oxygen</commodity>
                            <pref>1</pref>
                        </item>
                    </commodities>
                </stream>
                <stream>
                    <info>
                        <mixing_ratio>0.8815</mixing_ratio>
                        <buf_size>875</buf_size>
                    </info>
                    <commodities>
                        <item>
                            <commodity>enriched_U_fuelfab_prestore</commodity>
                            <pref>1</pref>
                        </item>
                    </commodities>
                </stream>
            </in_streams>
            <throughput>495</throughput>
            <out_buf_size>990</out_buf_size>
            <out_commod>reactor_fuel_prestore</out_commod>
        </Mixer>
    </config>
</facility>

<facility>
    <name>FuelFabShipper</name>
