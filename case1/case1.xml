// case 1

<simulation>
<control>
    <startmonth>1</startmonth>
    <startyear>2025</startyear>
    <dt>86400</dt>
    <duration>1040</duration>
    <explicit_inventory>0</explicit_inventory>
    <explicit_inventory_compact>0</explicit_inventory_compact>
</control>

<archetypes>
    <spec><lib>cycamore</lib><name>Source</name></spec>
    <spec><lib>cycamore</lib><name>Storage</name></spec>
    <spec><lib>cycamore</lib><name>Sink</name></spec>
    <spec><lib>cycamore</lib><name>Enrichment</name></spec>
    <spec><lib>cycamore</lib><name>Separations</name></spec>
    <spec><lib>cycamore</lib><name>Mixer</name></spec>
    <spec><lib>cycamore</lib><name>FuelFab</name></spec>
    <spec><lib>cycamore</lib><name>Reactor</name></spec>
    <spec><lib>agents</lib><name>NullRegion</name></spec>
    <spec><lib>agents</lib><name>NullInst</name></spec>
</archetypes>

<facility>
    <name>FSource</name>
    <config>
        <Source>
            <outcommod>fluorine</outcommod>
            <outrecipe>f</outrecipe>
        </Source>
    </config>
</facility>

<facility>
    <name>OSource</name>
    <config>
        <Source>
            <outcommod>oxygen</outcommod>
            <outrecipe>o</outrecipe>
        </Source>
    </config>
</facility>

<facility>
    <name>OFSink</name>
    <config>
        <Sink>
            <in_commods>
                <val>extra_fluorine</val>
                <val>extra_oxygen</val>
                <val>silica</val>
            </in_commods>
        </Sink>
    </config>
</facility>

<facility>
    <name>ForeignUF6Source</name>
    <config>
        <Source>
            <outcommod>UF6</outcommod>
            <outrecipe>UF6_feed</outrecipe>
        </Source>
    </config>
</facility>

<facility>
    <name>EnrichmentReceiver</name>
    <config>
        <Storage>
            <in_commods>
                <val>UF6</val>
            </in_commods>
            <out_commods>
                <val>UF6_received</val>
            </out_commods>
            <residence_time>1</residence_time>
            <throughput>12501</throughput>
            <max_inv_size>25002</max_inv_size>
        </Storage>
    </config>
</facility>

<facility>
    <name>EnrichmentStripU</name>
        <config>
        <Separations>
            <feed_commods>
                <val>UF6_received</val>
            </feed_commods>
            <feed_recipe>UF6_feed</feed_recipe>
            <feedbuf_size>17140</feedbuf_size>
            <throughput>8570</throughput>
            <leftover_commod>extra_fluorine</leftover_commod>
            <leftoverbuf_size>1e199</leftoverbuf_size>
            <streams>
                <item>
                    <commod>enrichment_feed</commod>
                    <info>
                        <buf_size>14532</buf_size>
                        <efficiencies>
                            <item>
                                <comp>U</comp>
                                <eff>0.99</eff>
                            </item>
                        </efficiencies>
                    </info>
                </item>
            </streams>
        </Separations>
    </config>
</facility>

<facility>
    <name>Enrichment</name>
    <config>
        <Enrichment>
            <feed_commod>enrichment_feed</feed_commod>
            <feed_recipe>U_enrichment_feed</feed_recipe>
            <product_commod>enriched_U_prestore</product_commod>
            <tails_commod>tails_prestore</tails_commod>
            <!--<tails_assay>0.003</tails_assay> -->
            <initial_feed>0</initial_feed>
            <max_feed_inventory>81900</max_feed_inventory>
            <max_enrich>0.034</max_enrich>
        </Enrichment>
    </config>
</facility>

<facility>
    <name>EnrichmentUF6Mixer</name>
    <config>
        <Mixer>
        <in_streams>
            <stream>
                <info>
                    <mixing_ratio>0.676</mixing_ratio>
                    <buf_size>1e10</buf_size>
                </info>
                <commodities>
                    <item>
                        <commodity>enriched_U_prestore</commodity>
                        <pref>1</pref>
                    </item>
                </commodities>
            </stream>
            <stream>
                <info>
                    <mixing_ratio>0.324</mixing_ratio>
                    <buf_size>1e10</buf_size>
                </info>
                <commodities>
                    <item>
                        <commodity>fluorine</commodity>
                        <pref>1</pref>
                    </item>
                </commodities>
            </stream>
        </in_streams>
        <out_commod>enriched_UF6_prestore</out_commod>
        <out_buf_size>1e10</out_buf_size>
        <throughput>12501</throughput>
        </Mixer>
    </config>
</facility>

<facility>
    <name>EnrichmentShipper</name>
    <config>
        <Storage>
            <in_commods><val>enriched_UF6_prestore</val></in_commods>
            <out_commods><val>enriched_UF6</val></out_commods>
            <in_recipe>leu_fuel</in_recipe>
            <residence_time>1</residence_time>
            <throughput>440</throughput>
            <max_inv_size>27324</max_inv_size>
            <sell_quantity>13662</sell_quantity>
        </Storage>
    </config>
</facility>

<facility>
    <name>DUTailings</name>
    <config>
        <Storage>
            <in_commods>
                <val>tails_prestore</val>
